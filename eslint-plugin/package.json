    "@microsoft/tsdoc-config": "0.13.5"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-3.5": "~0.8.4",
    "@rushstack/eslint-config": "~1.3.0",
    "@rushstack/heft": "^0.8.0",
    "@types/eslint": "6.1.3",
    "@types/estree": "0.0.39",
    "@types/heft-jest": "1.0.1",
    "@types/node": "10.17.5",
    "eslint": "~7.2.0"
  }
}
